from priority_queue import PriorityQueue

def main():
    pq = PriorityQueue()
    pq.add_task("Task A", 3)
    pq.add_task("Task B", 5)
    pq.add_task("Task C", 1)

    print("Tasks by priority:")
    while True:
        task = pq.get_next_task()
        if not task:
            break
        print(task)

if __name__ == "__main__":
    main()